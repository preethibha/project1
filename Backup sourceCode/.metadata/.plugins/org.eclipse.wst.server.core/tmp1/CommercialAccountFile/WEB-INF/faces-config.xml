<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
	<application>
		<state-manager>com.ibm.faces.application.DevelopmentStateManager</state-manager>
	</application>
	<factory>
		<faces-context-factory>com.ibm.faces.context.AjaxFacesContextFactory</faces-context-factory>
		<render-kit-factory>com.ibm.faces.renderkit.AjaxRenderKitFactory</render-kit-factory>
	</factory>
	<managed-bean>
		<managed-bean-name>pc_CAFTemplate</managed-bean-name>
		<managed-bean-class>pagecode.templates.CAFTemplate</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_Index</managed-bean-name>
		<managed-bean-class>pagecode.Index</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_Contactchubb</managed-bean-name>
		<managed-bean-class>pagecode.Contactchubb</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<lifecycle>
		<phase-listener>com.ibm.faces.webapp.ValueResourcePhaseListener</phase-listener>
	</lifecycle>
    
	<managed-bean>
		<managed-bean-name>cafBean</managed-bean-name>
		<managed-bean-class>pagecode.bean.CAFBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_StandaloneAppTemplate</managed-bean-name>
		<managed-bean-class>pagecode.templates.StandaloneAppTemplate</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
</faces-config>