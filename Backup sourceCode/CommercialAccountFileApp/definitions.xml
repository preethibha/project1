<project name="CommercialAcctFileAppDef" basedir="." xmlns='antlib:org.apache.tools.ant'>

	<!-- Commercial Account File Project Definitions -->
	
	<!-- Commercial Account File Model project -->
	<property name="dependency.caf.model.jar" value="CommercialAccountFileDomainModel.jar" />
	<property name="dependency.caf.model.dir" value="${basedir}/../CommercialAccountFileDomainModel" />

	<!-- Commercial Account File Util project -->
	<property name="dependency.caf.util.jar" value="CommercialAccountFileUtil.jar" />
	<property name="dependency.caf.util.dir" value="${basedir}/../CommercialAccountFileUtil" />

	<!-- Commercial Account File Domain project -->
	<property name="dependency.caf.domain.jar" value="CommercialAccountFileDomain.jar" />
	<property name="dependency.caf.domain.dir" value="${basedir}/../CommercialAccountFileDomain" />

	<!-- Commercial Account File Service Client project -->
	<property name="dependency.caf.service.client.jar" value="CommercialAccountFileServiceClients.jar" />
	<property name="dependency.caf.service.client.dir" value="${basedir}/../CommercialAccountFileServiceClients" />	
	
