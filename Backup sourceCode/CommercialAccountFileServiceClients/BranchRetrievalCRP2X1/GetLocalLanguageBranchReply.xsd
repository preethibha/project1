	<xsd:import schemaLocation="ExtendedBranch.xsd" namespace="http://ei/core/extendedbranch">
	</xsd:import>
	<xsd:complexType name="GetLocalLanguageBranchReply">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="ExtendedBranch" type="bons0:ExtendedBranch">
