<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://chubb_corporate/get_access/telecomm_electronic_address" targetNamespace="http://chubb_corporate/get_access/telecomm_electronic_address">
	<xsd:complexType name="TelecommElectronicAddress">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="CommDeviceCode" type="xsd:string">
				<xsd:annotation>
          <xsd:documentation>This element contains a symbol that identifies a communication device.
The E1T00741 Corporate Table contains valid values for this element in the "COMMU_DVC_C" column. </xsd:documentation>
        </xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CommDeviceName" type="xsd:string">
				<xsd:annotation>
          <xsd:documentation>This element contains the name of a communication device.
The E1T00741 Corporate Table contains valid values for this element in the "COMMU_DVC_NA" column. </xsd:documentation>
        </xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CommDeviceValue" type="xsd:string">
				<xsd:annotation>
          <xsd:documentation>This element contains the actual value used to contact an individual.  Examples would be actual email address (jdoe@yahoo.com) or telephone number.</xsd:documentation>
        </xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="ContactUsageTypeCode" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>This element contains a code that identifies how the contact information is to be used, such as business, billing, emergency, or personal.</xsd:documentation>
        </xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ContactUsageTypeName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>This element describes how the contact information is to be used, such as business, billing, emergency, or personal.</xsd:documentation>
        </xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="CommContactPreferenceOrderNumber" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>This element contains the order of preference of the communication devices.</xsd:documentation>
        </xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
<xsd:element name="TelecommElectronicAddress" type="TelecommElectronicAddress" /></xsd:schema>