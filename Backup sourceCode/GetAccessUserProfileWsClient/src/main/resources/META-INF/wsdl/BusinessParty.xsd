<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:bons5="http://chubb_corporate/get_access/individual_identifier" xmlns:bons1="http://chubb_corporate/get_access/individual_name" xmlns:bons2="http://chubb_corporate/get_access/typed_address" xmlns:bons3="http://chubb_corporate/get_access/telecomm_electronic_address" xmlns:bons4="http://chubb_corporate/get_access/business_party_characteristic" xmlns="http://chubb_corporate/get_access/business_party" targetNamespace="http://chubb_corporate/get_access/business_party">
	<xsd:import schemaLocation="BusinessPartyCharacteristic.xsd" namespace="http://chubb_corporate/get_access/business_party_characteristic">
	</xsd:import>
	<xsd:import schemaLocation="TelecommElectronicAddress.xsd" namespace="http://chubb_corporate/get_access/telecomm_electronic_address">
	</xsd:import>
	<xsd:import schemaLocation="TypedAddress.xsd" namespace="http://chubb_corporate/get_access/typed_address">
	</xsd:import>
	<xsd:import schemaLocation="IndividualName.xsd" namespace="http://chubb_corporate/get_access/individual_name">
	</xsd:import>
	<xsd:import schemaLocation="IndividualIdentifier.xsd" namespace="http://chubb_corporate/get_access/individual_identifier">
	</xsd:import>






	<xsd:complexType name="BusinessParty">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="IndividualIdentifier" type="bons5:IndividualIdentifier">
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="IndividualName" type="bons1:IndividualName">
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="BPTypedAddress" type="bons2:TypedAddress">
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="BPTelecommElectronicAddress" type="bons3:TelecommElectronicAddress">
			</xsd:element>
			<xsd:element minOccurs="0" name="BPDurableKey" type="xsd:string">
				<xsd:annotation>
          <xsd:documentation>This element contains an unique identifier assigned to an instance of a business party for its entire lifetime.</xsd:documentation>
        </xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BPTypeCode" type="xsd:string">
				<xsd:annotation>
          <xsd:documentation>This element identifies a type of business party.
The E1T01581 Corporate Table contains valid values for this element in the 'BP_TY_C' column. </xsd:documentation>
        </xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="BusinessPartyCharacteristic" type="bons4:BusinessPartyCharacteristic">
			</xsd:element>
			<xsd:element minOccurs="0" name="BusinessPartyName" type="xsd:string">
				<xsd:annotation>
          <xsd:documentation>This element contains the name of a business party.</xsd:documentation>
        </xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
<xsd:element name="BusinessParty" type="BusinessParty" /></xsd:schema>